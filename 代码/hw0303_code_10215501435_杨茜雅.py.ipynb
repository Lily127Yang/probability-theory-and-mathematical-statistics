    "import numpy as np\n",
    "\n",
    "n=0\n",
    "k=0\n",
    "while n<100000:\n",
    "  a=np.random.random()\n",
    "  b=np.random.random()\n",
    "  n += 1\n",
    "  if a+b <1.4:\n",
    "    k += 1\n",
    "p=k/n\n",
    "print(p)"
