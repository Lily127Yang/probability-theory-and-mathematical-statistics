   "source": [
    "from scipy import stats\n",
    "\n",
    "#显著性水平\n",
    "a = 0.975\n",
    "#自由度 \n",
    "df = 10\n",
    "\n",
    "# 单侧 左分位点\n",
    "T_a_left =stats.t.ppf(a,df)\n",
    "# 单侧 右分位点\n",
    "T_a_right =stats.t.isf(a,df)\n",
    "# 双侧分位点\n",
    "T_a_2 = stats.t.interval(1-a,df)\n",
    "\n",
    "print('单侧左分位点：',T_a_left)\n",
    "print('单侧右分位点：',T_a_right)\n",
    "print('双侧分位点：',T_a_2)\n"
   ]
