    "import numpy as np\n",
    "import math\n",
    "k=0\n",
    "n=0\n",
    "while n<10000000:\n",
    " x=np.random.random()\n",
    " y=np.random.random()\n",
    " n += 1\n",
    " if y <= ((math.exp(2*x-1)-math.exp(-1))/(math.exp(1)-math.exp(-1))):\n",
    "   k+=1\n",
    "print(2*(math.exp(1)-math.exp(-1))*k/n+2*math.exp(-1))\n",
    "   \n"
   ]
